{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "domains": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "doc_type": {
              "type": "string",
              "enum": [
                "web", "web/social_media", "web/forums", "web/news",
                "books", "books/literary/fiction", "books/literary/nonfiction", "books/textbooks",
                "reference/encyclopedic", "reference/dictionaries",
                "academic_papers/science", "academic_papers/humanities",
                "code/source_code", "code/documentation", "code/forums",
                "media/podcasts", "media/subtitles", "specific_datasets"
              ]
            },
            "data_size_gb": {
              "type": "number"
            },
            "documents": {
              "type": "number"
            },
            "tokens_billions": {
              "type": "number"
            },
            "features": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["doc_type", "tokens_billions"]
        }
      },
      "summary": {
        "type": "object",
        "properties": {
          "total_size": {
            "type": "number"
          },
          "mix": {
            "type": "object",
            "additionalProperties": {
              "type": "number"
            }
          }
        },
        "required": ["total_size", "mix"]
      }
    },
    "required": ["domains", "summary"]
  }
  