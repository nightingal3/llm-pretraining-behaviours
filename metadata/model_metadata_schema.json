{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "author": {
            "type": "string"
        },
        "sha": {
            "type": "string"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "last_modified": {
            "type": "string",
            "format": "date-time"
        },
        "private": {
            "type": "boolean"
        },
        "gated": {
            "type": "boolean"
        },
        "disabled": {
            "type": "boolean"
        },
        "downloads": {
            "type": "integer"
        },
        "likes": {
            "type": "integer"
        },
        "library_name": {
            "type": "string"
        },
        "tags": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "pipeline_tag": {
            "type": "string"
        },
        "mask_token": {
            "type": [
                "string",
                "null"
            ]
        },
        "card_data": {
            "type": "object"
        },
        "widget_data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "text": {
                        "type": "string"
                    }
                },
                "required": [
                    "text"
                ],
                "additionalProperties": false
            }
        },
        "model_index": {
            "type": [
                "string",
                "null"
            ]
        },
        "config": {
            "type": "object",
            "properties": {
                "architectures": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "model_type": {
                    "type": "string"
                },
                "tokenizer_config": {
                    "type": "object"
                }
            },
            "required": [
                "architectures",
                "model_type",
                "tokenizer_config"
            ],
            "additionalProperties": false
        },
        "transformers_info": {
            "type": "object",
            "properties": {
                "auto_model": {
                    "type": "string"
                },
                "custom_class": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "pipeline_tag": {
                    "type": "string"
                },
                "processor": {
                    "type": "string"
                }
            },
            "required": [
                "auto_model",
                "custom_class",
                "pipeline_tag",
                "processor"
            ],
            "additionalProperties": false
        },
        "safetensors": {
            "type": "object",
            "properties": {
                "parameters": {
                    "type": "object",
                    "patternProperties": {
                        "^[A-Z0-9]+$": {
                            "type": "integer"
                        }
                    },
                    "additionalProperties": false
                },
                "total": {
                    "type": "integer"
                }
            },
            "required": [
                "parameters",
                "total"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "id",
        "author",
        "sha",
        "created_at",
        "last_modified",
        "private",
        "gated",
        "disabled",
        "downloads",
        "likes",
        "library_name",
        "tags",
        "pipeline_tag",
        "mask_token",
        "card_data",
        "widget_data",
        "model_index",
        "config",
        "transformers_info",
        "safetensors"
    ],
    "additionalProperties": false
}